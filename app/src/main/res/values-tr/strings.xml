  <string name="notification_title_region_not_available">Seçilmiş bölge kullanılamıyor</string>
  <!--Message shown in notification informing the user that selected region is not currently available-->
  <string name="notification_text_region_not_available">Lütfen Psiphon uygulamasını açıp başka bir bölge seçin.</string>
  <!--"VPN stopped" notification title-->
  <string name="notification_title_vpn_revoked">Psiphon VPN durdu</string>
  <!--Message shown in notification informing the user that VPN has been disconnected by another app or revoked by the user-->
  <string name="notification_text_vpn_revoked">Psiphon VPN bağlantısı siz ya da başka bir uygulama tarafından kesildi.</string>
